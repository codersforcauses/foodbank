{
  "name": "client",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build:css && react-scripts start",
    "test": "react-scripts test",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "prebuild": "NODE_ENV=production yarn build:css",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "storybook": "yarn build:css && start-storybook -p 6006 -s public",
    "build-storybook": "NODE_ENV=production yarn build:css && build-storybook -s public"
  },
  "dependencies": {
    "@headlessui/react": "0.3.0",
    "axios": "0.21.1",
    "formik": "2.2.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.2",
    "swr": "0.4.2",
    "web-vitals": "1.1.0",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.3.1",
    "@storybook/addon-actions": "6.3.1",
    "@storybook/addon-essentials": "6.3.1",
    "@storybook/addon-links": "6.3.1",
    "@storybook/node-logger": "6.3.1",
    "@storybook/preset-create-react-app": "3.1.5",
    "@storybook/react": "6.3.1",
    "@storybook/theming": "6.3.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.3",
    "@types/jest": "26.0.20",
    "@types/node": "12.19.14",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "autoprefixer": "9.8.6",
    "babel-loader": "8.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.19.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "postcss": "7.0.35",
    "postcss-cli": "8.3.0",
    "react-snap": "1.23.0",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.1.3"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
